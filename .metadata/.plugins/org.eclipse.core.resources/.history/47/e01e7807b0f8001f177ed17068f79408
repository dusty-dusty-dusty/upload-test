package ch03;

public class WhileEX01_p108 {

	public static void main(String[] args) {
		//1 2 3 4 5
		int i=1;
		for( ; ; ) {
				if(i<=5) {//4<=5
					System.out.println(i);//1 2 3 4 5 
			}//if
				
			i++;
			if(i==6) {
				break;//반복문 => for문 종료
			}
			System.out.println("if문끝");
		}//for
		System.out.println("for문끝");
		//System.out.println("for문끝");
	}//main

}
