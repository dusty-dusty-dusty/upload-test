/*5단 출력
5*1=5 
5*2=10
...
5*9=45
*/


public class ForEx03_p115 {

	public static void main(String[] args) {
		int dan = 5;
		for(int i=1; i<=9; ++i ) {
			System.out.println(dan+"*"+i+"="+(dan*i));
		}
	}

}
