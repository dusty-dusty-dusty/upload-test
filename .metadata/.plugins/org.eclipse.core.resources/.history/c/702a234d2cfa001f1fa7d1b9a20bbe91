package ch07_array;

/*
 - 배열 항목에는 또 다른 배열이 대입된 배열
 - '[]'
 */





public class TwoDimension01_p219 {
	
	public static void main(String[] args) {
		//타입[] 배열명 = new타입[];
		/*배열명 = new타입[크기];
		  배열명=new 타입[크기];*/
		//타입[] 배열명 = new타입[크기];
		
		//배열생성시 초기화
		//타입[] 배열명 = new타입[]{값1,값2,....값n};
		//타입[] 배열명 = new타입{값1,값2,......};
		
		//배열생성시 초기화
		//5명의 학생이 수학점수
		int[] scores = {90,91,92,93,94};
		//System.out.println("scores="+scores);//[I@3d012ddd
		int len=scores.length;
		//System.out.println("학생수(scores.length)="+scores.length);
		for(int i=0;i<len;i++) {
			System.out.printf("%3d점 " ,scores[i]);
		}
		System.out.println("\w ㅁㅁ");//빈줄		
	}
}
