package ch02;

//character(51p)
public class Ex06_51p {

	public static void main(String[] args) {
		char c1 = 'a';
		System.out.println("char c1="+c1);
		
		int c11 = 'a';
		System.out.println("c11="+c11);//아스키코드
		
		
		char c2 = 'A';
		System.out.println("char c2="+c2);
		int c22 = 'A';
		System.out.println("int c22="+c22);//아스키코드
		
		char c3 = '1';
		System.out.println("char c3="+c3);
		char c4 = '!';
		System.out.println("char c4="+c4);
		int c44 = '!';
		System.out.println("int c44="+c44); //아스키코드
		
		int c55 = 'ひ';
		System.out.println("int c55="+c55); //아스키코드
		
	}

}
