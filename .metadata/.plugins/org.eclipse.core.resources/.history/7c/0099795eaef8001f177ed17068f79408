package ch03;

public class WhileEX01_p108 {

	public static void main(String[] args) {
		//1 2 3 4 5
		int i=1;
		for( ; ; ) {
			i++;
			if(i<=5) {//4<=5
			   System.out.println(i);//1 2 3 4 5 
			   i++;
			}//if
			i
			System.out.println("if문끝");
		}//for
		//System.out.println("for문끝");
		//System.out.println("for문끝");
	}//main

}
